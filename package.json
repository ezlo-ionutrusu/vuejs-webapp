{
  "name": "web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:skin:orm": "cross-env process.env.VUE_APP_SKIN='orm' process.env.VUE_APP_ENV='ezlo' vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "node-sass": "^4.12.0",
    "pako": "^1.0.10",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1",
    "websocket-rpc-client": "^0.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass-loader": "^7.1.0",
    "vue-svg-loader": "^0.12.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "linebreak-style": 0,
      "no-param-reassign": 0,
      "max-len": 0,
      "import/no-named-as-default": 0,
      "no-underscore-dangle": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "proxy": "http://192.168.11.58:81",
  "appSettings": {
    "defaultSkin": "default"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
